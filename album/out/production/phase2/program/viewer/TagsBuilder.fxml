<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="program.controller.TagsBuilderController">
   <children>
      <ListView fx:id="allTagsList" layoutX="115.0" layoutY="31.0" prefHeight="195.0" prefWidth="259.0" />
      <Button fx:id="addToAllBtn" layoutX="403.0" layoutY="116.0" mnemonicParsing="false" onAction="#addTagToAllImages" prefHeight="26.0" prefWidth="159.0" text="Add To All Images" />
      <Button fx:id="addBtn" layoutX="391.0" layoutY="256.0" mnemonicParsing="false" onAction="#add" prefHeight="26.0" prefWidth="159.0" text="Add To Tag Set" />
      <Button fx:id="deleteBtn" layoutX="403.0" layoutY="158.0" mnemonicParsing="false" onAction="#delete" prefHeight="26.0" prefWidth="159.0" text="Delete From Tag Set" />
      <TextField fx:id="addTagTxt" layoutX="115.0" layoutY="255.0" prefHeight="27.0" prefWidth="259.0" promptText="Please enter the tag you want to add" />
      <Button fx:id="addToThisBtn" layoutX="403.0" layoutY="72.0" mnemonicParsing="false" onAction="#addTagToThisImg" prefHeight="26.0" prefWidth="159.0" text="Add To This Image" />
      <Label fx:id="singleImgResult" alignment="CENTER" layoutX="115.0" layoutY="294.0" prefHeight="17.0" prefWidth="223.0" textAlignment="CENTER" textFill="RED" />
      <Label fx:id="allImgResult" contentDisplay="CENTER" layoutX="115.0" layoutY="311.0" prefHeight="17.0" prefWidth="284.0" textFill="RED" />
   </children>
</AnchorPane>
